{
    "manifest_version": 3,
    "name": "Auto Save Dynamic PDF",
    "version": "1.0",
    "description": "Automaticky ukládá dynamicky generované PDF soubory na základě URL.",
    "permissions": ["downloads", "webRequest", "webRequestBlocking"],
    "host_permissions": ["https://pms.previo.app/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Auto Save Dynamic PDF"
    }
  }
  